html
  head
    title Show List
  body
    include template
    h3  #{message}
    -if(Object.keys(list).length>0)
      table(border='1', width='500')
        tr
          th Roll No
          th Name
          th Percentage
        -each val in list
         tr
           td #{val.rollNo}
           td #{val.name}
           td #{val.percentage}
